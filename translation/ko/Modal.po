msgctxt "Modal/&ErrorModalIgnoreErrorTitle"
msgid "Ignore this Error"
msgstr "오류 무시"

msgctxt "Modal/&ErrorModalQuitTitle"
msgid "Quit the Game"
msgstr "게임 종료"

msgctxt "Modal/&ErrorModalTitle"
