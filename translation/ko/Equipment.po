msgctxt "Equipment/&WeaponTagHeavyTitle"
msgid "Heavy"
msgstr "중량형"

msgctxt "Equipment/&WeaponTagLightTitle"
msgid "Light"
msgstr "경량형"

msgctxt "Equipment/&WeaponTagLoadingTitle"
msgid "Loading"
msgstr "장전 필요"

msgctxt "Equipment/&WeaponTagReachTitle"
msgid "Reach"
msgstr "장병기"

msgctxt "Equipment/&WeaponTagThrownTitle"
msgid "Thrown"
msgstr "투척"

msgctxt "Equipment/&WeaponTagTwoHandedTitle"
