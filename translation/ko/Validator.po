msgctxt "Validator/&ParameterMonsterTitle"
msgid "Monster"
msgstr "몬스터"

msgctxt "Validator/&ParameterMonsterDescription"
msgid "Select a monster for this validator"
msgstr "확인도구를 사용할 몬스터를 선택하세요"

msgctxt "Validator/&ParameterItemTitle"
msgid "Item"
msgstr "아이템"

msgctxt "Validator/&ParameterItemDescription"
msgid "Select an item for this validator"
msgstr "확인도구를 사용할 아이템을 선택하세요"

msgctxt "Validator/&ParameterMonsterQuantityTitle"
msgid "How many?"
msgstr "수치?"

msgctxt "Validator/&ParameterMonsterQuantityDescription"
msgid "Determines the quantity of the specified element"
msgstr "지정 요소의 값을 확인합니다"

msgctxt "Validator/&ParameterItemQuantityTitle"
msgid "How many?"
msgstr "수치?"

msgctxt "Validator/&ParameterItemQuantityDescription"
msgid "Determines the quantity of the specified element"
msgstr "지정 요소의 값을 확인합니다"

msgctxt "Validator/&ParameterLocationTitle"
msgid "Location"
msgstr "위치"

msgctxt "Validator/&ParameterLocationDescription"
msgid "The title of the location to track"
msgstr "추적할 위치의 이름"

msgctxt "Validator/&ParameterRestTypeTitle"
msgid "Short=0 Long=1"
msgstr "짧은 휴식=0 긴 휴식=1"

msgctxt "Validator/&ParameterRestTypeDescription"
msgid "The type of expected rest"
msgstr "취할 휴식의 유형"

msgctxt "Validator/&ParameterSpellNameTitle"
msgid "Spell Name"
msgstr "주문 이름"

msgctxt "Validator/&ParameterSpellNameDescription"
msgid ""
"The exact internal name (in English & enforcing caps) of the spell to specify"
msgstr "지정할 주문의 정확한 내부 이름(영어 & 강제 캡)"

msgctxt "Validator/&ParameterCompletionDelayTitle"
msgid "Delay (s)"
msgstr "지연시간 (초)"

msgctxt "Validator/&ParameterCompletionDelayDescription"
msgid ""
"The delay in seconds before this is completed. Decimal values are allowed"
msgstr "작업이 완료되기 전에 지연되는 시간 (초). 10진수로 입력하세요"

msgctxt "Validator/&ParameterVariableTitle"
msgid "Variable"
msgstr "변수"

msgctxt "Validator/&ParameterVariableDescription"
msgid "The variable to test"
msgstr "테스트할 변수값"

msgctxt "Validator/&ParameterVariableTestVariableTitle"
msgid "Variable"
msgstr "변수"

msgctxt "Validator/&ParameterVariableTestVariableDescription"
msgid "The variable to evaluate"
msgstr "평가할 변수값"

msgctxt "Validator/&ParameterVariableTestTypeTitle"
msgid "Test Type"
msgstr "테스트 유형"

msgctxt "Validator/&ParameterVariableTestTypeDescription"
msgid "The test type to perform on the variable"
msgstr "변수에 적용할 테스트의 유형"

msgctxt "Validator/&ParameterVariableTestTypeIsEqualToTitle"
msgid "Is Equal To"
msgstr "은(는) 다음과 일치합니다"

msgctxt "Validator/&ParameterVariableTestTypeIsEqualToDescription"
msgid "Valid if the variable value matches the specified value"
msgstr "변수값이 지정값과 일치하면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestTypeIsDifferentFromTitle"
msgid "Is Different From"
msgstr "은(는) 다음과 불일치합니다"

msgctxt "Validator/&ParameterVariableTestTypeIsDifferentFromDescription"
msgid "Valid if the variable value differs from the specified value"
msgstr "변수값이 지정값과 불일치하면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestTypeIsGreaterThanTitle"
msgid "Is Greater Than"
msgstr "은(는) 다음 값 이상입니다"

msgctxt "Validator/&ParameterVariableTestTypeIsGreaterThanDescription"
msgid "Valid if the variable value is superior or equal to the specified value"
msgstr "변수값이 지정값 이상이면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestTypeIsGreaterStrictlyThanTitle"
msgid "Is Strictly Greater Than"
msgstr "은(는) 다음 값보다 매우 큽니다"

msgctxt "Validator/&ParameterVariableTestTypeIsGreaterStrictlyThanDescription"
msgid ""
"Valid if the variable value is strictly greater than the specified value"
msgstr "변수값이 지정값보다 매우 크면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestTypeInferiorToTitle"
msgid "Is Less Than"
msgstr "은(는) 다음 값 이하입니다"

msgctxt "Validator/&ParameterVariableTestTypeInferiorToDescription"
msgid ""
"Valid if the variable value is less than or equal to the specified value"
msgstr "변수값이 지정값 이하면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestTypeIsInferiorStrictlyToTitle"
msgid "Is Strictly Less Than"
msgstr "은(는) 다음 값보다 매우 작습니다"

msgctxt "Validator/&ParameterVariableTestTypeIsInferiorStrictlyToDescription"
msgid "Valid if the variable value is strictly less than the specified value"
msgstr "변수값이 지정값보다 매우 작으면 나오는 결과"

msgctxt "Validator/&ParameterVariableTestValueIntTitle"
msgid "Test Value"
msgstr "테스트 상수 값"

msgctxt "Validator/&ParameterVariableTestValueIntDescription"
msgid "The integer value to compare in the test"
msgstr "테스트에서 비교할 상수 값"

msgctxt "Validator/&ParameterVariableTestValueBoolTitle"
msgid "Test Value"
msgstr "테스트 논리 값"

msgctxt "Validator/&ParameterVariableTestValueBoolDescription"
msgid "The Boolean value to compare in the test"
msgstr "테스트에서 비교할 논리연산자 값"

msgctxt "Validator/&ParameterVariableTestValueStringTitle"
msgid "Test Value"
msgstr "테스트 문자열 값"

msgctxt "Validator/&ParameterVariableTestValueStringDescription"
msgid "The string value to compare in the test"
msgstr "테스트에서 비교할 문자열 값"
