msgctxt "Caption/&JumpFromCaption"
msgid "Select a spot to jump from."
msgstr "점프를 시작할 위치를 선택하세요."

msgctxt "Caption/&JumpToCaption"
msgid "Select a spot to jump to."
msgstr "점프 목표 지점을 선택하세요."

msgctxt "Caption/&LandingPositionCaption"
msgid "Select a place to climb to."
msgstr "올라갈 위치를 선택하세요."

msgctxt "Caption/&LevitateDestinationCaption"
msgid "Select a levitation destination for your target."
msgstr "착지 지점을 선택하세요."

msgctxt "Caption/&OutOfRangeCaption"
msgid "You are out of range!"
msgstr "범위를 벗어났습니다!"

msgctxt "Caption/&ProxyActionTitle"
msgid "Magical Effect Control"
msgstr "마법 효과 제어"

msgctxt "Caption/&ProxyAttackCaption"
msgid "Select a target to attack with the magic effect."
msgstr "마법 효과로 공격할 대상을 선택하세요."

msgctxt "Caption/&ProxyMoveAndAttackCaption"
msgid "Select destination to move the magical effect or a target to attack."
msgstr "마법 효과나 공격할 대상을 이동시킬 곳을 고르세요."

msgctxt "Caption/&ProxyMoveCaption"
msgid "Select a destination to move the magic effect."
msgstr "마법 효과를 이동시킬 곳을 고르세요."

msgctxt "Caption/&SelectPositionMultipleCaption"
msgid "Select {0} positions"
msgstr "위치 {0}개를 선택하세요"

msgctxt "Caption/&SelectPositionSingleCaption"
msgid "Select a position"
msgstr "위치를 선택하세요"

msgctxt "Caption/&ShapeConeDirectionCaption"
msgid "Select a direction for the cone of effect."
msgstr "원뿔형 효과의 방향을 선택하세요."

msgctxt "Caption/&ShapeCubePositionCaption"
msgid "Select a position for the cube of effect."
msgstr "육면체형 효과의 위치를 선택하세요."

msgctxt "Caption/&ShapeCylinderPositionCaption"
msgid "Select a position for the cylinder of effect."
msgstr "원통형 효과의 위치를 선택하세요."

msgctxt "Caption/&ShapeLineDirectionCaption"
msgid "Select a direction for the line of effect."
msgstr "직선 효과의 방향을 선택하세요."

msgctxt "Caption/&ShapeSpherePositionCaption"
msgid "Select a position for the sphere of effect."
msgstr "효과 적용 대상 범위(구형)를 선택하세요."

msgctxt "Caption/&ShapeWallLineExtremityCaption"
msgid "Select an end for the wall effect."
msgstr "벽 효과의 끝 지점을 선택하세요."

msgctxt "Caption/&ShapeWallLineInvalid"
msgid "The line of effect cannot be placed here."
msgstr "직선 효과는 여기에 놓을 수 없습니다."

msgctxt "Caption/&ShapeWallLineOriginCaption"
msgid "Select an origin for the wall effect."
msgstr "벽 효과의 시작 지점을 선택하세요."

msgctxt "Caption/&ShapeWallLineOutOfLengthCaption"
msgid "The line of effect is too long!"
msgstr "직선 효과가 너무 깁니다!"

msgctxt "Caption/&ShapeWallRingPositionCaption"
msgid "Select a position for the circle of effect."
msgstr "원형 효과의 위치를 선택하세요."

msgctxt "Caption/&ShoveSelectDestinationCaption"
msgid "Push the creature back, or knock it down?"
msgstr "크리쳐를 뒤로 밀치겠습니까, 아니면 쓰러뜨리겠습니까?"

msgctxt "Caption/&ShoveSelectTargetCaption"
msgid "Select a target to shove."
msgstr "밀칠 대상을 선택하세요."

msgctxt "Caption/&ShoveTargetCannotPushAwayCaption"
msgid "No room to push the target away."
msgstr "대상을 밀칠 공간이 없습니다."

msgctxt "Caption/&ShoveTargetImmuneCaption"
msgid "Target is immune to shoving."
msgstr "대상은 밀치기에 면역입니다."

msgctxt "Caption/&ShoveTargetOutOfReachCaption"
msgid "Target is out of reach."
msgstr "대상이 범위 밖에 있습니다."

msgctxt "Caption/&ShoveTargetProneCaption"
msgid "You cannot shove a prone target!"
msgstr "넘어진 대상은 밀칠 수 없습니다!"

msgctxt "Caption/&ShoveTargetTooBigCaption"
msgid "Target is too big for this creature to shove"
msgstr "밀쳐내기에는 대상이 너무 큽니다."

#, fuzzy
#| msgid "Click to awaken this sleeping character."
msgctxt "Caption/&TargetAwakenDescription"
msgid "Awaken this sleeping character."
msgstr "이 캐릭터를 잠에서 깨우려면 클릭하세요."

msgctxt "Caption/&TargetFilteringAllyCreature"
msgid "Ally Creature"
msgstr "우호적 크리쳐"

msgctxt "Caption/&TargetFilteringCreature"
msgid "Creature"
msgstr "크리쳐"

msgctxt "Caption/&TargetFilteringEffectProxy"
msgid "Magical Effect"
msgstr "마법 효과"

msgctxt "Caption/&TargetFilteringEnemyCreature"
msgid "Enemy Creature"
msgstr "적 크리쳐"

msgctxt "Caption/&TargetFilteringGadget"
msgid "Object"
msgstr "오브젝트"

msgctxt "Caption/&TargetMultipleCaption"
msgid ""
"Select {1} targets ({0}).\n"
"\n"
"<#B0B0B0>You can select each target multiple times.</color>"
msgstr ""
"{1} 대상 ({0})을(를) 지정합니다.\n"
"\n"
"<#B0B0B0>하나의 대상을 여러 번 지정할 수 있습니다.</color>"

msgctxt "Caption/&TargetMultipleUniqueCaption"
msgid ""
"Select {1} targets ({0}).\n"
"<#B0B0B0>You can select each target only once.</color>"
msgstr ""
"{1} 대상 ({0})을 지정합니다.\n"
"<#B0B0B0>하나의 대상은 한 번만 지정할 수 있습니다.</color>"

msgctxt "Caption/&TargetProximityMultipleCaption"
msgid "<#B0B0B0>Targets must be within {0} cells of each other.</color>"
msgstr "<#B0B0B0>대상들은 서로 {0}칸 이내에 있어야 합니다.</color>"

msgctxt "Caption/&TargetProximitySingleCaption"
msgid "<#B0B0B0>Targets must be next to each other.</color>"
msgstr "<#B0B0B0>대상들은 서로 인접해야 합니다.</color>"

msgctxt "Caption/&TargetRequiredConditionCaption"
msgid "<#B0B0B0>The target must be \"{0}.\"</color>"
msgstr "<#B0B0B0>대상은 \"{0}\"이어야 합니다.</color>"

msgctxt "Caption/&TargetRequiredCreatureTypeCaption"
msgid "<#B0B0B0>Targets must be {0}"
msgstr "<#B0B0B0>대상은 {0}이어야 합니다"

msgctxt "Caption/&TargetRequiredUnarmoredCaption"
msgid "<#B0B0B0>Targets must be unarmored."
msgstr "<#B0B0B0>대상은 무장 해제 상태여야 합니다."

msgctxt "Caption/&TargetShareUniqueCaption"
msgid ""
"Select any number of targets ({0}) to share the effect ({1}).\n"
"\n"
"<#B0B0B0>You can select each target only once.</color>"
msgstr ""
"({1}) 효과를 공유할 대상을 최대 ({0})명 만큼 선택합니다.\n"
"\n"
"<#B0B0B0>각 대상은 한 번만 선택할 수 있습니다.</color>"

msgctxt "Caption/&TargetSingleCaption"
msgid "Select a target ({0}).\n"
msgstr "대상({0})을(를) 선택합니다.\n"

#, fuzzy
