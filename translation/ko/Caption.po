msgctxt "JumpFromCaption"
msgid "Select a spot to jump from."
msgstr "점프를 시작할 위치를 선택하세요."

msgctxt "JumpToCaption"
msgid "Select a spot to jump to."
msgstr "점프 목표 지점을 선택하세요."

msgctxt "LandingPositionCaption"
msgid "Select a place to climb to."
msgstr "올라갈 위치를 선택하세요."

msgctxt "LevitateDestinationCaption"
msgid "Select a levitation destination for your target."
msgstr "착지 지점을 선택하세요."

msgctxt "OutOfRangeCaption"
msgid "You are out of range!"
msgstr "범위를 벗어났습니다!"

msgctxt "ProxyActionTitle"
msgid "Magical Effect Control"
msgstr "마법 효과 제어"

msgctxt "ProxyAttackCaption"
msgid "Select a target to attack with the magic effect."
msgstr "마법 효과로 공격할 대상을 선택하세요."

msgctxt "ProxyMoveAndAttackCaption"
msgid "Select destination to move the magical effect or a target to attack."
msgstr "마법 효과나 공격할 대상을 이동시킬 곳을 고르세요."

msgctxt "ProxyMoveCaption"
msgid "Select a destination to move the magic effect."
msgstr "마법 효과를 이동시킬 곳을 고르세요."

msgctxt "SelectPositionMultipleCaption"
msgid "Select {0} positions"
msgstr "위치 {0}개를 선택하세요"

msgctxt "SelectPositionSingleCaption"
msgid "Select a position"
msgstr "위치를 선택하세요"

msgctxt "ShapeConeDirectionCaption"
msgid "Select a direction for the cone of effect."
msgstr "원뿔형 효과의 방향을 선택하세요."

#, fuzzy
msgctxt "ShapeCubePositionCaption"
msgid "Select a position for the cube of effect."
msgstr "육면체형 효과의 위치를 선택하세요."

#, fuzzy
msgctxt "ShapeCylinderPositionCaption"
msgid "Select a position for the cylinder of effect."
msgstr "원통형 효과의 위치를 선택하세요."

#, fuzzy
msgctxt "ShapeLineDirectionCaption"
msgid "Select a direction for the line of effect."
msgstr "선형 효과의 방향을 선택하세요."

#, fuzzy
msgctxt "ShapeSpherePositionCaption"
msgid "Select a position for the sphere of effect."
msgstr "구형 효과의 위치를 선택하세요."

msgctxt "ShapeWallLineExtremityCaption"
msgid "Select an end for the wall effect."
msgstr "벽 효과의 끝 지점을 선택하세요."

#, fuzzy
msgctxt "ShapeWallLineInvalid"
msgid "The line of effect cannot be placed here."
msgstr "선형 효과는 여기에 놓을 수 없습니다."

msgctxt "ShapeWallLineOriginCaption"
msgid "Select an origin for the wall effect."
msgstr "벽 효과의 시작 지점을 선택하세요."

#, fuzzy
msgctxt "ShapeWallLineOutOfLengthCaption"
msgid "The line of effect is too long!"
msgstr "선형 효과가 너무 깁니다!"

#, fuzzy
msgctxt "ShapeWallRingPositionCaption"
msgid "Select a position for the circle of effect."
msgstr "원형 효과의 위치를 선택하세요."

#, fuzzy
msgctxt "ShoveSelectDestinationCaption"
msgid "Push the creature back, or knock it down?"
msgstr "크리쳐를 밀치겠습니까, 아니면 쓰러뜨리겠습니까?"

msgctxt "ShoveSelectTargetCaption"
msgid "Select a target to shove."
msgstr "밀칠 대상을 선택하세요."

msgctxt "ShoveTargetCannotPushAwayCaption"
msgid "No room to push the target away."
msgstr "대상을 밀칠 공간이 없습니다."

msgctxt "ShoveTargetImmuneCaption"
msgid "Target is immune to shoving."
msgstr "대상은 밀치기에 면역입니다."

msgctxt "ShoveTargetOutOfReachCaption"
msgid "Target is out of reach."
msgstr "대상이 범위 밖에 있습니다."

msgctxt "ShoveTargetProneCaption"
msgid "You cannot shove a prone target!"
msgstr "넘어진 대상은 밀칠 수 없습니다!"

msgctxt "ShoveTargetTooBigCaption"
msgid "Target is too big for this creature to shove"
msgstr "밀치기에 대상이 너무 큽니다."

#, fuzzy
msgctxt "TargetAwakenDescription"
msgid "Click to awaken this sleeping character."
msgstr "잠에 든 이 캐릭터를 깨우려면 클릭하세요."

#, fuzzy
msgctxt "TargetFilteringAllyCreature"
msgid "Ally Creature"
msgstr "우호적 크리쳐"

msgctxt "TargetFilteringCreature"
msgid "Creature"
msgstr "크리쳐"

msgctxt "TargetFilteringEffectProxy"
msgid "Magical Effect"
msgstr "마법 효과"

msgctxt "TargetFilteringEnemyCreature"
msgid "Enemy Creature"
msgstr "적 크리쳐"

msgctxt "TargetFilteringGadget"
msgid "Object"
msgstr "오브젝트"

msgctxt "TargetMultipleCaption"
msgid ""
"Select {1} targets ({0}).\n"
"\n"
"<#B0B0B0>You can select each target multiple times.</color>"
msgstr ""
"{1} 대상 ({0})을(를) 지정합니다.\n"
"\n"
"<#B0B0B0>하나의 대상을 여러 번 지정할 수 있습니다.</color>"

msgctxt "TargetMultipleUniqueCaption"
msgid ""
"Select {1} targets ({0}).\n"
"<#B0B0B0>You can select each target only once.</color>"
msgstr ""
"{1} 대상 ({0})을 지정합니다.\n"
"<#B0B0B0>하나의 대상은 한 번만 지정할 수 있습니다.</color>"

msgctxt "TargetProximityMultipleCaption"
msgid "<#B0B0B0>Targets must be within {0} cells of each other.</color>"
msgstr "<#B0B0B0>대상들은 서로 {0}칸 이내에 있어야 합니다.</color>"

msgctxt "TargetProximitySingleCaption"
msgid "<#B0B0B0>Targets must be next to each other.</color>"
msgstr "<#B0B0B0>대상들은 서로 인접해야 합니다.</color>"

msgctxt "TargetRequiredConditionCaption"
msgid "<#B0B0B0>The target must be \"{0}.\"</color>"
msgstr "<#B0B0B0>대상은 \"{0}\"이어야 합니다.</color>"

msgctxt "TargetRequiredCreatureTypeCaption"
msgid "<#B0B0B0>Targets must be {0}"
msgstr "<#B0B0B0>대상은 {0}이어야 합니다"

msgctxt "TargetRequiredUnarmoredCaption"
msgid "<#B0B0B0>Targets must be unarmored."
msgstr "<#B0B0B0>대상은 무장 해제 상태여야 합니다."

#, fuzzy
msgctxt "TargetShareUniqueCaption"
msgid ""
"Select any number of targets ({0}) to share the effect ({1}).\n"
"\n"
"<#B0B0B0>You can select each target only once.</color>"
msgstr ""
"({1}) 효과를 공유할 대상을 최대 ({0})명 만큼 선택합니다.\n"
"\n"
"<#B0B0B0>각 대상은 한 번만 선택할 수 있습니다.</color>"

msgctxt "TargetSingleCaption"
msgid "Select a target ({0}).\n"
msgstr "대상 ({0})을(를) 선택합니다.\n"

msgctxt "TargetStabilizeDescription"
msgid ""
"Click to attempt to stabilize this dying character, with a WIS (Medicine) "
"ability check."
msgstr "지혜 (의학) 판정을 통해 죽어가는 캐릭터를 안정화시키려면 클릭하십시오."

#, fuzzy
msgctxt "CannotBePlacedOnCharacterCaption"
msgid "You cannot cast this spell directly on a character."
msgstr "이 주문은 캐릭터에게 직접 시전할 수 없습니다."

#, fuzzy
msgctxt "TargetsMustPerceiveCasterCaption"
msgid "Targets must perceive the source of the effect."
msgstr "대상은 효과의 근원을 인식하여야 합니다."

msgctxt "PositionMustBePerceivedCaption"
msgid "The position is not perceived."
msgstr "위치가 인식되지 않습니다."

msgctxt "TargetRequiredSizeCaption"
msgid "Target size must be: {0}"
msgstr "대상 크기가 {0}이어야 합니다."

#, fuzzy
msgctxt "InviteOptionalAllyCaption"
msgid "Touch an optional ally to bring along."
msgstr "함께할 동료를 선택하세요."

msgctxt "TelekinesisSelectDirection"
msgid "Select a direction in which to move this creature."
msgstr "이 생물을 옮길 방향을 선택하세요."

msgctxt "EditorNewPropPlacementCaption"
msgid ""
"Select a valid position to place this new prop\n"
"Hold \"Shift\" to place multiple props of this type"
msgstr ""
"새로운 물체를 배치할 유효한 위치를 선택하세요.\n"
"같은 물체 여러 개를 배치하려면 \"쉬프트 키\"를 누른 채로 클릭하세요."

msgctxt "EditorExistingPropPlacementCaption"
msgid "Select a new position for this prop"
msgstr "이 물체를 배치할 새로운 위치를 선택하세요."

msgctxt "EditorNewGadgetPlacementCaption"
msgid ""
"Select a valid position to place this new gadget\n"
"Hold \"Shift\" to place multiple gadgets of this type"
msgstr ""
"새로운 장치를 배치할 위치를 선택하세요.\n"
"\"쉬프트 키\"를 누르고 있으면 여러 개를 배치합니다."

msgctxt "EditorExistingGadgetPlacementCaption"
msgid "Select a new position for this gadget"
msgstr "이 장치의 새로운 위치를 선택하세요."

msgctxt "EditorSelectedRoomCaption"
msgid "Selected Room: {0}"
msgstr "선택한 방: {0}"

msgctxt "EditorSelectedPropCaption"
msgid "Selected Prop: {0}"
msgstr "선택한 물체: {0}"

msgctxt "EditorSelectedGadgetCaption"
msgid "Selected Gadget: {0}"
msgstr "선택한 장치: {0}"

msgctxt "GadgetParametersHeaderTitle"
msgid "Gadget Parameters:"
msgstr "장치 옵션:"

msgctxt "GadgetParametersSelectorTitle"
msgid "Select"
msgstr "선택"

msgctxt "GadgetParametersSelectorDescription"
msgid ""
"Click to select a corresponding element (monster, item, loot pack) in the "
"database"
msgstr "데이터베이스에서 해당하는 요소 (몬스터, 아이템, 전리품 꾸러미)를 선택하려면 클릭하세요"

msgctxt "GadgetParametersAddTitle"
msgid "Add Element"
msgstr "요소 추가"

msgctxt "GadgetParametersAddDescription"
msgid "Add a corresponding element (monster, item, loot pack) to the list"
msgstr "목록에 해당 요소(몬스터, 아이템, 전리품 꾸러미) 추가"

msgctxt "GadgetParametersRemoveDescription"
msgid "Remove the selected element"
msgstr "선택한 요소 제거"

msgctxt "GadgetParametersNoneTitle"
msgid "<Empty>"
msgstr "<비어 있음>"

msgctxt "EditorNewRoomPlacementCaption"
msgid ""
"Select a valid position to place this new room\n"
"Hold \"Shift\" to place multiple rooms of this type"
msgstr ""
"새로운 방을 배치할 유효한 위치를 선택하세요.\n"
"같은 방 여러 개를 배치하려면 \"쉬프트 키\"를 누른 채로 클릭하세요."

msgctxt "EditorExistingRoomPlacementCaption"
msgid "Select a new position for this room"
msgstr "방을 배치할 새로운 위치를 선택하세요."

msgctxt "GadgetParametersLocationSelectorDescription"
msgid "Click to select a destination location from a list"
msgstr "클릭하여 목록에서 목적지 위치를 선택하세요."

msgctxt "GadgetListAddTitle"
msgid "Add Gadget"
msgstr "장치 추가"

msgctxt "GadgetListAddDescription"
msgid "Allows you to click and a select a target gadget"
msgstr "대상 장치와 상호작용이 가능하게 합니다."

msgctxt "EditorSelectTargetGadgetDescription"
msgid "Select a target gadget"
msgstr "대상 장치 선택"

