msgctxt "BanditTrackDescription"
msgid "Human tracks - probably bandits. They look quite fresh."
msgstr "인간의 흔적 - 아마도 도적일 것입니다. 그들은 꽤 신선해 보입니다."

msgctxt "DisarmTrapDescription"
msgid "Click to try to disarm this trap."
msgstr "이 트랩을 해제하려면 클릭하십시오."

msgctxt "HardClimbDescription"
msgid "Click to try to climb."
msgstr "클릭하여 등반을 시도합니다."

msgctxt "LightDescription"
msgid "Click to light this light."
msgstr "이 조명을 켜려면 클릭합니다."

msgctxt "LockPickChestDescription"
msgid "Click to try to pick the lock on this chest."
msgstr "이 상자의 자물쇠를 선택하려면 클릭하세요."

msgctxt "LockPickDoorDescription"
msgid "Click to try to pick this door's lock."
msgstr "이 문의 자물쇠를 선택하려면 클릭하세요."

msgctxt "LootContainerDescription"
msgid "Click to loot this container."
msgstr "이 컨테이너를 약탈하려면 클릭하세요."

msgctxt "NpcInteractionDescription"
msgid "Click to interact with this character."
msgstr "클릭하면 이 캐릭터와 상호작용할 수 있습니다."

msgctxt "ObserveDescription"
msgid "Click to examine this object."
msgstr "이 개체를 검사하려면 클릭하십시오."

msgctxt "OpenCloseDoorDescription"
msgid "Click to open/close this door."
msgstr "이 문을 열거나 닫으려면 클릭하세요."

msgctxt "PushDescription"
msgid "Click to push this object."
msgstr "이 개체를 푸시하려면 클릭하세요."

msgctxt "StartLongRestDescription"
msgid "Click to start a long rest for the party."
msgstr "파티를 위한 긴 휴식을 시작하려면 클릭하세요."

msgctxt "TriggerTrapDescription"
msgid "Trigger collapse."
msgstr "붕괴를 방아쇠를 당깁니다."

msgctxt "UseDescription"
msgid "Click to use this object."
msgstr "이 개체를 사용하려면 클릭하세요."

msgctxt "GoblinTrackDescription"
msgid "Footprints… Probably goblins'. They look quite fresh."
msgstr "발자국… 아마 고블린이겠지. 그들은 꽤 신선해 보입니다."

msgctxt "UnknownTrackDescription"
msgid "Fresh tracks, probably those of the fanatics."
msgstr "신선한 트랙, 아마도 광신도의 트랙일 것입니다."

msgctxt "SorakTrackDescription"
msgid "Reptilian tracks… Soraks, maybe?"
msgstr "렙틸리언 트랙... 소락스, 아마도?"

msgctxt "QuickShoppingDescription"
msgid "Click to open quick shopping."
msgstr "빠른 쇼핑을 열려면 클릭하세요."

msgctxt "WaitDescription"
msgid "Click to wait for the moon to rise."
msgstr "달이 뜨기를 기다리려면 클릭하세요."

msgctxt "UseScavengersDescription"
msgid "Click to use the Scavenger System"
msgstr "스캐빈저 시스템을 사용하려면 클릭하세요."

msgctxt "TakeScimitarDescription"
msgid "Click to use the scimitar and the shield for this fight."
msgstr "이 전투에서 시미터와 방패를 사용하려면 클릭하세요."

msgctxt "TakeDaggersDescription"
msgid "Click to use the daggers for this fight."
msgstr "이 전투에서 단검을 사용하려면 클릭하세요."

msgctxt "TakeGreataxeDescription"
msgid "Click to use the greataxe for this fight."
msgstr "이 싸움에 대도끼를 사용하려면 클릭하세요."

msgctxt "LeaveArenaDescription"
msgid "Click to leave the arena and forfeit the duel."
msgstr "경기장을 떠나 결투를 포기하려면 클릭하십시오."

msgctxt "PlantDocumentDescription"
msgid "Plant Document"
msgstr "식물 문서"

msgctxt "Unlock_Door_With_Key_Description"
msgid "Use the adequate key to open the door."
msgstr "적절한 키를 사용하여 문을 엽니다."

msgctxt "NpcInteractionEnlistDescription"
msgid "Click to enlist this character as a companion"
msgstr "이 캐릭터를 동료로 참여시키려면 클릭하세요."
