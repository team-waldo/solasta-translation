msgctxt "Corrosive_Bolt_Title"
msgid "Corrosive Bolt"
msgstr "부식성 볼트"

msgctxt "Flash_Bolt_Title"
msgid "Flash Bolt"
msgstr "플래시 볼트"

msgctxt "AlchemicalProjectile_Description"
msgid "This projectile uses an alchemical flask for additional damage."
msgstr "이 발사체는 연금술 플라스크를 사용하여 추가 피해를 줍니다."

msgctxt "Flaming_Arrow_CraftingRecipe_Title"
