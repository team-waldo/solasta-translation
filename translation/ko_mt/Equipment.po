msgctxt "Corrosive_Arrow_Title"
msgid "Corrosive Arrow"
msgstr "부식성 화살"

msgctxt "Flash_Arrow_Title"
msgid "Flash Arrow"
msgstr "플래시 애로우"

msgctxt "Flaming_Bolt_Title"
