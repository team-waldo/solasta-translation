msgctxt "FailureFlagAlreadyWieldingLightSource"
msgid "You are already holding a light source."
msgstr "당신은 이미 광원을 들고 있습니다."

msgctxt "FailureFlagAttributesAssigned"
msgid "You have already assigned scores."
msgstr "이미 점수를 할당했습니다."

msgctxt "FailureFlagBlockedMove"
msgid "Path is blocked."
msgstr "경로가 차단되었습니다."

msgctxt "FailureFlagCannotAfford"
msgid "You cannot afford to spend that much."
msgstr "그렇게 많이 지출할 여유가 없습니다."

msgctxt "FailureFlagCannotAffordCosts"
msgid "You cannot afford to spend {0}."
msgstr "{0}을(를) 지출할 여유가 없습니다."

msgctxt "FailureFlagCannotCastAnySpell"
msgid "You cannot cast any spells at this time."
msgstr "지금은 어떤 주문도 시전할 수 없습니다."

msgctxt "FailureFlagCannotCastLightCantrip"
msgid "You cannot cast the light cantrip."
msgstr "빛 캔트립을 시전할 수 없습니다."

msgctxt "FailureFlagCannotRestTooOften"
msgid "You cannot take a long rest twice in the same day."
msgstr "같은 날 두 번 긴 휴식을 취할 수 없습니다."

msgctxt "FailureFlagCannotScribeScrolls"
msgid "This character cannot scribe scrolls into a spellbook."
msgstr "이 캐릭터는 두루마리를 마법서에 쓸 수 없습니다."

msgctxt "FailureFlagCannotUseAnyPower"
msgid "You cannot use any powers at this time."
msgstr "지금은 어떤 능력도 사용할 수 없습니다."

msgctxt "FailureFlagConditionNotMatching"
msgid "The target does not have the right condition."
msgstr "대상에 올바른 조건이 없습니다."

msgctxt "FailureFlagDeadTarget"
msgid "You cannot target a dead character."
msgstr "죽은 캐릭터를 대상으로 지정할 수 없습니다."

msgctxt "FailureFlagDocumentUnknownLanguage"
msgid "No one in the party is proficient with this language."
msgstr "파티원 중 누구도 이 언어에 능숙하지 않습니다."

msgctxt "FailureFlagFreeOnceActionSpent"
msgid "You have already spent your free/inventory action this round."
msgstr "이번 라운드에 이미 무료/재고 조치를 사용했습니다."

msgctxt "FailureFlagFreeOnceActionSpentNotOnBody"
msgid ""
"You have already spent your free/inventory action this round and this item "
"is not carried on your person."
msgstr "이번 라운드에 이미 무료/재고 조치를 사용했으며 이 항목은 귀하의 개인이 가지고 있지 않습니다."

msgctxt "FailureFlagFreeOnceActionUnavailable"
msgid "Your free/inventory action is not available."
msgstr "무료/인벤토리 작업을 사용할 수 없습니다."

msgctxt "FailureFlagFunctionDurationCooldown"
msgid "You must wait {0} before you can use this function again."
msgstr "이 기능을 다시 사용하려면 {0}을(를) 기다려야 합니다."

msgctxt "FailureFlagFunctionNotEnoughCharges"
msgid "The item does not have enough charges to use this function."
msgstr "항목에 이 기능을 사용하기에 충분한 요금이 없습니다."

msgctxt "FailureFlagFunctionOutOfIterations"
msgid "The item has run out of uses for this function."
msgstr "항목에 이 기능에 대한 사용이 다 되었습니다."

msgctxt "FailureFlagFunctionRequiresAttunement"
msgid "You must be attuned to this item (short rest) to use this."
msgstr "이것을 사용하려면 이 항목(짧은 휴식)에 맞춰야 합니다."

msgctxt "FailureFlagHardLockedDoor"
msgid "Door cannot be opened."
msgstr "문을 열 수 없습니다."

msgctxt "FailureFlagHardLockedDoorNoLock"
msgid "There is no visible lock. This door cannot be opened from here."
msgstr "보이는 자물쇠가 없습니다. 이 문은 여기에서 열 수 없습니다."

msgctxt "FailureFlagHardLockedDoorTutorialOrcHideout"
msgid "Must complete the quest objective before opening the door."
msgstr "문을 열기 전에 퀘스트 목표를 완료해야 합니다."

msgctxt "FailureFlagItemTargetTooFarAway"
msgid "Too far away to select an item."
msgstr "항목을 선택하기에는 너무 멀리 있습니다."

msgctxt "FailureFlagLockedCharacter"
msgid "This character is locked and cannot be changed."
msgstr "이 캐릭터는 잠겨 있으며 변경할 수 없습니다."

msgctxt "FailureFlagLockedDoor"
msgid "You need thieves' tools to pick a lock."
msgstr "자물쇠를 따려면 도둑의 도구가 필요합니다."

msgctxt "FailureFlagMainActionUnavailable"
msgid "You have spent your main action, or are not able to use it."
msgstr "당신은 당신의 주요 행동을 소비하거나 그것을 사용할 수 없습니다."

msgctxt "FailureFlagMaximumPersonalityFlags"
msgid "You have already selected the maximum number of personality flags."
msgstr "이미 최대 개수의 개성 플래그를 선택했습니다."

msgctxt "FailureFlagMissingConfigurationItem"
msgid "You are missing one or more items from this configuration."
msgstr "이 구성에서 하나 이상의 항목이 누락되었습니다."

msgctxt "FailureFlagNoCharacterCreated"
msgid "You have not created a character."
msgstr "캐릭터를 생성하지 않았습니다."

msgctxt "FailureFlagNoMainActionToExecute"
msgid "You have spent your main action."
msgstr "당신은 당신의 주요 행동을 소비했습니다."

msgctxt "FailureFlagNoPathToLocation"
msgid "No path found to navigate to this location."
msgstr "이 위치로 이동할 경로를 찾을 수 없습니다."

msgctxt "FailureFlagNoPerceptionOfTargetDescription"
msgid "You cannot see this target."
msgstr "이 대상을 볼 수 없습니다."

msgctxt "FailureFlagNoReachForTargetDescription"
msgid "You cannot reach the target."
msgstr "목표에 도달할 수 없습니다."

msgctxt "FailureFlagNoSavedGame"
msgid "No valid saved game found on this computer."
msgstr "이 컴퓨터에서 유효한 저장된 게임을 찾을 수 없습니다."

msgctxt "FailureFlagNoSuitableLightTarget"
msgid "You are not carrying any item suitable for the light cantrip."
msgstr "라이트 캔트립에 적합한 아이템을 가지고 있지 않습니다."

msgctxt "FailureFlagNotEnoughFood"
msgid "You are missing {0} unit(s) of food."
msgstr "{0}단위의 음식이 없습니다."

msgctxt "FailureFlagNotEnoughMove"
msgid "Not enough move."
msgstr "움직임이 충분하지 않습니다."

msgctxt "FailureFlagNotInBattle"
msgid "Not in battle."
msgstr "전투 중이 아닙니다."

msgctxt "FailureFlagNoTorchInInventory"
msgid "You have no torches in your inventory."
msgstr "인벤토리에 횃불이 없습니다."

msgctxt "FailureFlagOncePerTurnOnly"
msgid "Once per turn only."
msgstr "한 턴에 한 번만."

msgctxt "FailureFlagOnlyAfterALongRest"
msgid "You can only do this after a long rest."
msgstr "오랜 휴식 후에 만 ​​이 작업을 수행 할 수 있습니다."

msgctxt "FailureFlagPartyTooSpreadOut"
msgid "You must gather your party together."
msgstr "당신은 당신의 파티를 함께 모아야합니다."

msgctxt "FailureFlagProjectileBlocked"
msgid "Projectile is blocked."
msgstr "발사체가 차단됩니다."

msgctxt "FailureFlagProtectedCharacter"
msgid "This character cannot be deleted."
msgstr "이 캐릭터는 삭제할 수 없습니다."

msgctxt "FailureFlagScrollNotInSpellList"
msgid "The spell on this scroll is not on your spell list."
msgstr "이 두루마리의 주문은 당신의 주문 목록에 없습니다."

msgctxt "FailureFlagScrollNotOnSpellList"
msgid "This scroll is not compatible with this character's spell list."
msgstr "이 두루마리는 이 캐릭터의 주문 목록과 호환되지 않습니다."

msgctxt "FailureFlagSpellLevelTooHigh"
msgid "The spell level is too high."
msgstr "주문 수준이 너무 높습니다."

msgctxt "FailureFlagTargetAlreadySelected"
msgid "Target is already selected."
msgstr "대상이 이미 선택되었습니다."

msgctxt "FailureFlagTargetIncorrectCreatureFamily"
msgid "Target creature does not have the correct type."
msgstr "대상 생물의 유형이 올바르지 않습니다."

msgctxt "FailureFlagTargetIsAnEffectProxy"
msgid "Target is a magical effect."
msgstr "대상은 마법 효과입니다."

msgctxt "FailureFlagTargetIsNotACreature"
msgid "Target is not a creature."
msgstr "대상은 생물이 아닙니다."

msgctxt "FailureFlagTargetIsNotAGadget"
msgid "Target is not an object."
msgstr "대상은 개체가 아닙니다."

msgctxt "FailureFlagTargetMustBeUnarmored"
msgid "Target must be unarmored."
msgstr "목표는 무장하지 않은 상태여야 합니다."

msgctxt "FailureFlagTargetOutOfRange"
msgid "Target is out of range."
msgstr "대상이 범위를 벗어났습니다."

msgctxt "FailureFlagTargetTooFarFromOtherTargets"
msgid "Target is too far from other targets."
msgstr "대상이 다른 대상에서 너무 멀리 떨어져 있습니다."

msgctxt "FailureFlagTransferCannotCarry"
msgid "Out of space/too overloaded to carry this item."
msgstr "공간이 부족하거나 이 항목을 휴대하기에는 너무 과부하가 걸립니다."

msgctxt "FailureFlagTransferTooFarAway"
msgid "Too far away to transfer the item."
msgstr "항목을 전송하기에는 너무 멀리 있습니다."

msgctxt "FailureFlagTwoHandedWeaponEquiped"
msgid "You are wielding a two-handed weapon."
msgstr "당신은 양손 무기를 휘두르고 있습니다."

msgctxt "FailureFlagUnassignedCharacter"
msgid "You must assign all characters before you can proceed."
msgstr "계속하기 전에 모든 문자를 할당해야 합니다."

msgctxt "FailureFlagUnsafeRestLocation"
msgid "It is not safe to rest here."
msgstr "여기서 쉬는 것은 안전하지 않습니다."

msgctxt "FailureFlagUseItemActionUnavailable"
msgid "You cannot use the \"Use an Item\" action."
msgstr "\"아이템 사용\" 액션을 사용할 수 없습니다."

msgctxt "FailureFlagWrongLoadContextDescription"
msgid "You cannot load a game at this time."
msgstr "현재 게임을 로드할 수 없습니다."

msgctxt "FailureFlagWrongSaveContextDescription"
msgid "You cannot save the game at this time."
msgstr "현재 게임을 저장할 수 없습니다."

msgctxt "FeedbackText_OpenDoorLever"
msgid "The door is opened by a lever."
msgstr "문은 레버로 열립니다."

msgctxt "FailureFlagHardLockedDoorTOM"
msgid "This door is designed to be opened remotely."
msgstr "이 문은 원격으로 열 수 있도록 설계되었습니다."

msgctxt "FailureFlagRestNotPermitted"
msgid "This type of rest is not authorized right now"
msgstr "이 유형의 휴식은 현재 승인되지 않았습니다."

msgctxt "FailureFlagOutOfPerceptionRange"
msgid "This target is too far away to be perceived."
msgstr "이 목표는 인식하기에는 너무 멀리 떨어져 있습니다."

msgctxt "FailureFlagPartyCannotCastSpell"
msgid "No party member can cast this spell or has it prepared."
msgstr "파티원은 이 주문을 시전하거나 준비할 수 없습니다."

msgctxt "FailureFlagProxyNeedsCasterVision"
msgid "The caster needs to perceive the target."
msgstr "시전자는 대상을 인지해야 합니다."

msgctxt "FailureFlagVerbalComponentCannotSpeak"
msgid "Unable to speak and perform verbal spell components."
msgstr "구두 주문 구성 요소를 말하고 수행할 수 없습니다."

msgctxt "FailureFlagSomaticComponentRestrained"
msgid "Restrained and unable to perform somatic spell components."
msgstr "체세포 주문 구성 요소를 억제하고 수행할 수 없습니다."

msgctxt "FailureFlagSomaticComponentHandsFull"
msgid "No free hand to perform somatic spell components."
msgstr "체세포 주문 구성 요소를 수행할 자유 손이 없습니다."

msgctxt "FailureFlagMaterialComponentMissingFocus"
msgid "Must wield/wear a Spell Focus or Component Pouch."
msgstr "Spell Focus 또는 Component Pouch를 착용/착용해야 합니다."

msgctxt "FailureFlagMaterialComponentMissingSpecific"
msgid "Missing a(n) \"{0}\" item with a value of at least {1}."
msgstr "값이 {1} 이상인 \"{0}\" 항목이 누락되었습니다."

msgctxt "FailureFlagFunctionRequiresIdentification"
msgid "You must identify this item to use it."
msgstr "사용하려면 이 항목을 식별해야 합니다."

msgctxt "FailureFlagFunctionNotInCombat"
msgid "Cannot use this function in combat."
msgstr "전투 중에는 이 기능을 사용할 수 없습니다."

msgctxt "FailureFlagNoValidRemainingPages"
msgid ""
"None of this character's spellbooks has any room left for the {0} pages "
"required for the {1} spell."
msgstr "이 캐릭터의 주문서에는 {1} 주문에 필요한 {0} 페이지를 위한 공간이 남아 있지 않습니다."

msgctxt "FailureFlagAllSpellbooksAlreadyScribed"
msgid ""
"All the spellbooks carried by this character already contain the {0} spell."
msgstr "이 캐릭터가 가지고 있는 모든 주문서는 이미 {0} 주문을 포함하고 있습니다."

msgctxt "FailureFlagRecipeAlreadyKnown"
msgid "You already know this crafting option."
msgstr "당신은 이미 이 제작 옵션을 알고 있습니다."

msgctxt "FailureFlagNoRecipeWithTool"
msgid "You do not know any crafting options that use this tool."
msgstr "이 도구를 사용하는 제작 옵션을 모릅니다."

msgctxt "FailureTeleportNoPortalAtOrigin"
msgid "The current party location does not have a Major Gate."
msgstr "현재 파티 위치에는 메이저 게이트가 없습니다."

msgctxt "FailureTeleportNoPortalAtDestination"
msgid "The destination location has a Major Gate."
msgstr "목적지 위치에 메이저 게이트가 있습니다."

msgctxt "FailureFlagDeityMarkAreadyPresent"
msgid "The mark of this character's deity is only present on this item"
msgstr "이 캐릭터의 신의 표식은 이 아이템에만 있습니다."

msgctxt "FailureFlagHardLockedDoor_Key"
msgid "You need the adequate key to open this door."
msgstr "이 문을 열려면 적절한 열쇠가 필요합니다."

msgctxt "FailureFlagInsufficientSorceryPoints"
msgid "Not enough sorcery points"
msgstr "마법 점수가 충분하지 않습니다."

msgctxt "FailureFlagUnavailableSpellSlot"
msgid "No spell slot of this level is available"
msgstr "이 수준의 주문 슬롯을 사용할 수 없습니다."

msgctxt "FailureFlagNoSavingThrowAvailable"
msgid "The spell does not provide a saving throw"
msgstr "주문은 내성 굴림을 제공하지 않습니다"

msgctxt "FailureFlagInvalidSpellActionType"
msgid "The spell does not the correct action type"
msgstr "주문이 올바른 행동 유형이 아닙니다."

msgctxt "FailureFlagCraftingItemTooHeavy"
msgid ""
"The crafted item is too heavy to fit into the crafting character's inventory"
msgstr "제작된 아이템이 너무 무거워서 제작 캐릭터의 인벤토리에 들어갈 수 없습니다."

msgctxt "FailureFlagMissingUserLocation"
msgid "You must select a user location"
msgstr "사용자 위치를 선택해야 합니다."

msgctxt "FailureLocationMustHaveEntrance"
msgid "The location must have an entrance"
msgstr "위치에 입구가 있어야 합니다."

msgctxt "FailureLocationMustContainOneRoom"
msgid "The location must contain at least one room"
msgstr "위치에 방이 하나 이상 있어야 합니다."

msgctxt "FailureLocationInconsistentCharacterLevels"
msgid "The min./max. character levels are inconsistent"
msgstr "최소/최대 캐릭터 레벨이 일정하지 않음"

msgctxt "FailureGadgetNotInRoom"
msgid "The gadget must be placed inside an existing room"
msgstr "가젯은 기존 방 안에 배치해야 합니다."

msgctxt "FailureGadgetOutOfRoomBounds"
msgid "The gadget is outside the bounds of this room"
msgstr "가젯이 이 방의 경계를 벗어났습니다."

msgctxt "FailureGadgetOverlapExisting"
msgid "The gadget is overlapping another gadget or prop"
msgstr "가젯이 다른 가젯 또는 소품과 겹치고 있습니다."

msgctxt "FailurePropNotInRoom"
msgid "The prop must be placed inside an existing room"
msgstr "소품은 기존 방 안에 배치해야 합니다."

msgctxt "FailurePropOutOfRoomBounds"
msgid "The prop is outside the bounds of this room"
msgstr "소품이 이 방의 경계를 벗어났습니다."

msgctxt "FailurePropOverlapExisting"
msgid "The prop is overlapping another gadget or prop"
msgstr "소품이 다른 가제트 또는 소품과 겹치고 있습니다."

msgctxt "FailureInvalidPosition"
msgid "Invalid Position"
msgstr "잘못된 위치"

msgctxt "FailureGadgetInvalidParameters"
msgid "Some of the parameters of this gadget are invalid"
msgstr "이 가젯의 일부 매개변수가 잘못되었습니다."

msgctxt "FailureRoomOverlapExisting"
msgid "The room is overlapping another room"
msgstr "방이 다른 방과 겹치고 있습니다."

msgctxt "FailureRoomOutOfBounds"
msgid "The room is outside the bounds of the location"
msgstr "방이 위치 범위 밖에 있습니다."

msgctxt "FailureFlagCopyrightInfringementTitle"
msgid "The title of the location uses the copyrighted term \"{0}\""
msgstr "위치 제목은 저작권 용어 \"{0}\"를 사용합니다."

msgctxt "FailureFlagCopyrightInfringementDescription"
msgid "The description of the location uses the copyrighted term \"{0}\""
msgstr "위치에 대한 설명은 저작권 용어 \"{0}\"를 사용합니다."

msgctxt "FailureFlagCopyrightInfringementLoreGadget"
msgid "The \"{0}\" gadget uses the copyrighted term \"{1}\""
msgstr "\"{0}\" 가젯은 \"{1}\" 저작권 용어를 사용합니다."

msgctxt "FailureFlagMustMoveToRotate"
msgid "This object is not square. You must move it to rotate it"
msgstr "이 물체는 정사각형이 아닙니다. 회전하려면 움직여야 합니다."

msgctxt "FailureFlagMustSaveToPlaytest"
msgid "You must save your location before you can playtest it"
msgstr "플레이 테스트를 하기 전에 위치를 저장해야 합니다."

msgctxt "EditorCannotSelectSourceGadget"
msgid "Cannot select the source gadget"
msgstr "소스 가젯을 선택할 수 없습니다."

msgctxt "FailureFlagOnlyInBattle"
msgid "Only in battle"
msgstr "전투에서만"

msgctxt "FailureLocationMustHaveEntranceIndex0"
msgid "The location must have one of its entrances with entrance index 0"
msgstr "위치에는 입구 인덱스가 0인 입구 중 하나가 있어야 합니다."

msgctxt "FailureGadgetInvalidPlacement"
msgid "The gadget placement is invalid"
msgstr "가젯 배치가 잘못되었습니다."

msgctxt "FailurePropInvalidPlacement"
msgid "The prop placement is invalid"
msgstr "소품 배치가 잘못되었습니다."

msgctxt "FailureEditorCannotSelectIncompatibleGadget"
msgid "Cannot be activated"
msgstr "활성화할 수 없음"

msgctxt "FailureEditorCannotSelectDuplicateGadget"
msgid "Already activated by this gadget"
msgstr "이 가젯에 의해 이미 활성화됨"

msgctxt "FailureCampaignMustContainOneLocation"
msgid "The campaign must have at least one location"
msgstr "캠페인에는 하나 이상의 위치가 있어야 합니다."

msgctxt "FailureCampaignMustHaveStartLocation"
msgid "The campaign must have a start location"
msgstr "캠페인에는 시작 위치가 있어야 합니다."

msgctxt "FailureCampaignInconsistentCharacterLevels"
msgid "Inconsistent min/max character levels"
msgstr "일관되지 않은 최소/최대 캐릭터 레벨"

msgctxt "FailureCampaignEmptyTitle"
msgid "Invalid Title"
msgstr "잘못된 제목"

msgctxt "FailureCampaignDuplicateTitle"
msgid "You must enter a valid title for your user campaign"
msgstr "사용자 캠페인의 유효한 제목을 입력해야 합니다."

msgctxt "FailureCampaignNothingModified"
msgid "Duplicate Title"
msgstr "중복 제목"

msgctxt "FailureFlagInvalidSingleTarget"
msgid "The spell must have a single target"
msgstr "주문에는 단일 대상이 있어야 합니다."

msgctxt "FailureFlagDurationInferiorTo1Min"
msgid "The spell duration is less than 1 minute"
